@model IEnumerable<PendalePharmacy.Models.Faq>


<div style="padding:10px">


    <a class="btn btn-primary" href="@Url.Action("Create")">Create New</a>
</div>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.comments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.comments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FaqID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FaqID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FaqID })
        </td>
    </tr>
}

</table>
